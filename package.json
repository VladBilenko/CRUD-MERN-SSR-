    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.8.0",
    "cookie-parser": "~1.4.4",
    "copy-webpack-plugin": "^4.2.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "3.4.2",
    "date-fns": "^2.14.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.2.0",
