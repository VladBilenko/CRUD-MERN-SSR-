    "express": "^4.16.4",
    "express-validator": "^6.5.0",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "history": "4.10.1",
    "html-webpack-plugin": "4.0.0-beta.11",
    "http2": "latest",
    "identity-obj-proxy": "3.0.0",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-style-loader": "^5.1.0",
